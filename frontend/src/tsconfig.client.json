{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["DOM", "ES2020"],
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "strict": false,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      },
      "noEmit": true
    },
    "include": ["src"]
  }
  